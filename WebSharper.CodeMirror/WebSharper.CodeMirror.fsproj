<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>netstandard2.0</TargetFrameworks>
    <WebSharperProject>Extension</WebSharperProject>
    <WebSharperSourceMap>false</WebSharperSourceMap>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>
  <Target Name="EmbedResources" BeforeTargets="BeforeBuild">
    <ReadLinesFromFile File="$(MSBuildThisFileDirectory)..\.temp\res-full.txt" >
      <Output TaskParameter="Lines" ItemName="CodeMirrorResources" />
    </ReadLinesFromFile>
    <ItemGroup>
      <EmbeddedResource Include="@(CodeMirrorResources)" />
    </ItemGroup>
  </Target>
  <ItemGroup>
    <Compile Include="Main.fs" />
  </ItemGroup>
  <Import Project="..\.paket\Paket.Restore.targets" />
</Project>
